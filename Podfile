# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'
use_frameworks!

target '速递医疗 病人端' do
    # 网络访问
    pod 'Moya'
    # Toast
    pod 'SVProgressHUD'
    pod 'Toast-Swift'
    # 界面约束
    pod 'SnapKit'
    # json 转换
    pod 'ObjectMapper'
    # Md5
    pod 'SwiftHash', git: 'https://github.com/onmyway133/SwiftHash'
    pod 'Kingfisher'
end
